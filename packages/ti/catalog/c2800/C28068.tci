/*
 *  ======== C28068.tci ========
 *
 */

utils.importFile("ti/catalog/c2800/C28069.tci");

/*
 *  ======== C28068 ========
 */
function C28068(name, parent)
{
    /* Since 28068 and 28069 are same from the configuration point of view,
     * instead of duplicating code, the 28068 device is configured using the
     * code for 28069 and then only deviceName is changed.
     */

    C28069.apply(this, [name, parent]);
    this.deviceName = "28068";
}
