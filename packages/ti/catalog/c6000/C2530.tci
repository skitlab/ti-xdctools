/*
 *  ======== C2530.tci ========
 *
 */

utils.importFile("ti/catalog/c6000/C2430.tci");

/*
 *  ======== C2530 ========
 */
function C2530(name, parent)
{
    /* Since OMAP2430 and OMAP2530 are configured in the same way, OMAP2530
     * is configured using the code for OMAP2430.
     */

    C2430.apply(this, [name, parent]);
    this.deviceName = "2530";
}
